{
    "name": "sharik709/laravel-github",
    "description": "It will make it easier for laravel application to use github api",
    "type": "library",
    "require" : {
        "tan-tan-kanarek/github-php-client": "^1.0",
        "illuminate/support" : "^5.8.4"
    },
    "require-dev": {
    },
    "license": "MIT",
    "autoload" : {
        "psr-4": {
            "LaravelGithub\\": "src",
            "LaravelGithub\\Test\\" : "src/tests/"
        }
    },
    "authors": [
        {
            "name": "Sharik shaikh",
            "email": "shaikhsharik709@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
